FROM

IPD_REGISTRATION_REPORT_TABLE A,
PATIENT_INDOOR_TABLE B,
PATIENT_MASTER_TABLE C

WHERE
A.IPD_ID_FK=B.ID AND
B.PATIENT_ID_FK=C.ID;
GO
