CONVERT(CHAR(5), A.OBSERVATION_TIME, 108) AS OBSERVATION_TIME,
B.PATIENT_GENERAL_HEALTH
FROM
IPD_OBSERVATION_TABLE A,
PATIENT_GENERAL_HEALTH B
WHERE
A.PATIENT_HEALTH_CONDITION=B.ID;
GO
