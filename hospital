CREATE PROC [dbo].[sp_Mobile_ForgotAppointmentToken]

@patientPhoneNumber varchar(max),
@patientDOB datetime

AS

SELECT TOP 1
A.APPOINTMENT_TOKEN
FROM
APPOINTMENT_TABLE A,
PATIENT_MASTER_TABLE B
WHERE
A.PATIENT_ID_FK=B.ID AND
B.PATIENT_DOB=@patientDOB AND
B.PATIENT_PRIMARY_PHONE=@patientPhoneNumber;
GO
