CONVERT(CHAR(5), [LEAVE_TO_TIME], 108) AS TO_TIME 
FROM EMPLOYEE_LEAVE_TABLE
WHERE
LEAVE_TO_DATE>=GETDATE() AND
EMPLOYEE_ID_FK=@empId;
