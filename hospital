GO
CREATE TABLE [dbo].[EMPLOYEE_SCHEDULE_TABLE](
	[ID] [int] IDENTITY(1,1) NOT NULL,
	[EMPLOYEE_ID_FK] [int] NOT NULL,
	[MON_SESS1_FROM] [datetime] NULL,
	[MON_SESS1_TO] [datetime] NULL,
	[MON_SESS2_FROM] [datetime] NULL,
