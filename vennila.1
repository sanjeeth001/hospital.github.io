      err=1;
      $('#doctor_id').focus();
    }

    if(err==0){

      $.ajax({
          url: ajaxurl,
          type: "POST",
          data: "act=checkIsDocAvail&doctorId="+doc_id,
          success: function(data){ 
              //alert(data);
              if(data=='1'){
                window.open("http://schedule.kmchhospital.com/onlinereg/docappredirect.html?CompId="+loc_id+"&DocId="+doc_id,"_self");
